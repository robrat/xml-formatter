root = true

[*.*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.java]
indent_style = space
indent_size = 2
max_line_length = 100
continuation_indent_size = 4

[*.xml]
indent_style = space
indent_size = 4
max_line_length = 100
continuation_indent_size = 8
